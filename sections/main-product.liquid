															class='countdown_block_seconds {{ block.settings.countdown_block_font }}'
														>
															{% if seconds.size > 1 %}
																{{ seconds }}
															{% else %}
																{{ 0 | append: seconds }}
															{% endif %}
														</span>
														{{- 'general.countdown.seconds' | t -}}
													</div>
												</div>
											</div>
										{%- endif -%}
									</div>
								</div>
							</countdown-timer>
						{%- when 'dropdown_information' -%}
							<div class="about">
                <div class="about__accordion-item" {{ block.shopify_attributes }}>
                  <button
                    class="about__accordion-toggle{% if block.settings.default_open %} active{% endif %} button button--unstyled"
                    area-label="toggle"
                  >
                    <span class="h5">{{ block.settings.heading | escape }}</span>
                    <div class="about__accordion-toggle-icon">
                     	{% render 'icon-minus' %}
											{% render 'icon-plus' %}
                    </div>
                  </button>
                  <div class="about__accordion-description rte {% if settings.rte_type == 'custom' %} rte__container{% endif %} {% if block.settings.default_open %} active{% endif %}">
                    {%- if block.settings.characteristics and product.metafields.characteristics.name.value.size > 0 -%}
                      {%- for item in product.metafields.characteristics.name.value -%}
                        <div class="about__row">
                          <span>{{ item }}</span>
                          <span>{{ product.metafields.characteristics.description.value[forloop.index0] }}</span>
                        </div>
                      {%- endfor -%}
