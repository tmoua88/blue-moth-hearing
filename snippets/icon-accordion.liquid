{% liquid
	case size
		when 'small'
			assign icon_size = 33
		when 'medium'
			assign icon_size = 66
		when 'large'
			assign icon_size = 88
		else
			assign icon_size = 66
	endcase
%}
{% if custom_icon != blank or custom_icon_dark != blank %}
	{% if custom_icon != blank %}
		{% assign icon_height = icon_size | divided_by: custom_icon.aspect_ratio %}
		{{
			custom_icon
			| image_url: width: 3840
			| image_tag:
				loading: 'lazy',
				width: icon_size,
				height: icon_height,
				sizes: '100vw (min-width: 767px) 50vw',
				widths: '375, 550, 750, 1100, 1500, 1780',
				style: 'object-fit: cover',
				class: 'header__heading-logo header__logo-light'
		}}
	{% endif %}
	{% if custom_icon_dark != blank %}
		{% assign icon_height = icon_size
			| divided_by: custom_icon_dark.aspect_ratio
		%}
		{{
			custom_icon_dark
			| image_url: width: 3840
			| image_tag:
				loading: 'lazy',
				width: icon_size,
				height: icon_height,
				style: 'object-fit: cover',
				sizes: '100vw (min-width: 767px) 50vw',
				widths: '375, 550, 750, 1100, 1500, 1780',
				class: 'header__heading-logo header__heading-logo--overlay'
		}}
	{% endif %}
{% elsif icon != 'none' %}
	<svg
		class='icon icon-accordion color-foreground-{{ settings.icon }} icon-{{ size }}'
		aria-hidden='true'
		focusable='false'
		role='presentation'
		viewBox='0 0 30 30'
		fill='none'
		xmlns='http://www.w3.org/2000/svg'
		width='{{ icon_size }}'
		height='{{ icon_size }}'
	>
